#ifndef GPHOTO2_CONFIG_H
#define GPHOTO2_CONFIG_H

/* Define to 1 if translation of program messages to the user's native language is requested. */
#cmakedefine ENABLE_NLS 1
/* The gettext domain we're using */
#cmakedefine GETTEXT_PACKAGE "@GETTEXT_PACKAGE@"
#cmakedefine GETTEXT_LOCALEDIR "@GETTEXT_LOCALEDIR@"

/* Name of package */
#define PACKAGE "@PACKAGE@"
/* Define to the version of this package. */
#cmakedefine PACKAGE_VERSION "@PACKAGE_VERSION@"

/* Allow multiple applications to access same camera device by opening and closing devices during each command. */
#cmakedefine HAVE_MULTI 1

/* Whether we compile with libexif support. */
#cmakedefine HAVE_LIBEXIF 1
/* Whether we use a version of libexif with ExifData.ifd[[]] */
#cmakedefine HAVE_LIBEXIF_IFD 1
/* Whether we compile with gdlib support. */
#cmakedefine HAVE_LIBGD 1
/* Define if we found LIBJPEG and its headers. */
#cmakedefine HAVE_LIBJPEG 1
/* Whether we compile with libxml-2.0 support. */
#cmakedefine HAVE_LIBXML2 1
/* Define if you have the iconv() function. */
#cmakedefine HAVE_ICONV 1

/* Define to 1 if you have the `localtime_r' function. */
#cmakedefine HAVE_LOCALTIME_R 1
/* Whether struct tm has tm_gmtoff field. */
#cmakedefine HAVE_TM_GMTOFF 1
/* Define to 1 if you have the `gmtime_r' function. */
#cmakedefine HAVE_GMTIME_R 1

/* Define to 1 if you have the <locale.h> header file. */
#cmakedefine HAVE_LOCALE_H 1
/* Define to 1 if you have the <limits.h> header file. */
#cmakedefine HAVE_LIMITS_H 1

/* Define to 1 if you have the <langinfo.h> header file. */
#cmakedefine HAVE_LANGINFO_H 1
/* Define to 1 if you have the <sys/statvfs.h> header file. */
